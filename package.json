    "@tailwindcss/line-clamp": "^0.2.2",
    "@tailwindcss/typography": "^0.4.1",
    "framer-motion": "^5.0.0",
    "next": "12.0.1",
    "next-themes": "^0.0.15",
    "phosphor-react": "^1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/tailwindcss": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
