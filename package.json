{
    "name": "next-start",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "preview": "wrangler dev",
        "deploy": "wrangler deploy",
        "build:workers": "opennextjs-cloudflare build",
        "typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
        "cleanup": "pnpm dlx del-cli .{next,open-next,wrangler} out",
        "update-deps": "npm-check-updates",
        "format": "biome format . --write",
        "lint": "biome lint . --write",
        "check": "biome check . --write",
        "typecheck": "tsc -b"
    },
    "dependencies": {
        "consola": "^3.4.2",
        "lucide-react": "^0.544.0",
        "next": "15.5.3",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "std-env": "^3.9.0",
        "tailwind-variants": "^3.1.1",
        "twistail-utils": "^0.11.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.4",
        "@cloudflare/workers-types": "^4.20250919.0",
        "@opennextjs/cloudflare": "1.8.3",
        "@tailwindcss/postcss": "^4.1.13",
        "@types/node": "24.5.2",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "npm-check-updates": "^18.2.1",
        "postcss": "^8.5.6",
        "postcss-load-config": "^6.0.1",
        "tailwindcss": "^4.1.13",
        "tailwindcss-motion": "^1.1.1",
        "typescript": "5.7.3",
        "wrangler": "^4.38.0"
    },
    "packageManager": "pnpm@10.17.0",
    "pnpm": {
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "esbuild",
            "sharp"
        ]
    }
}
