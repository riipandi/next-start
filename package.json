{
    "name": "next-start",
    "version": "13.5.3",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "lint": "TIMING=1 next lint",
        "lint:fix": "next lint --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "docker:build": "docker build . --no-cache -t $npm_package_name:$npm_package_version -t $npm_package_name:latest",
        "docker:run": "docker run --rm -it --env-file .env -p 3000:3000 --name $npm_package_name $npm_package_name:latest",
        "docker:shell": "docker exec -it $npm_package_name /bin/sh",
        "docker:logs": "docker logs -f $npm_package_name",
        "docker:stop": "docker stop $npm_package_name"
    },
    "dependencies": {
        "@t3-oss/env-nextjs": "^0.9.2",
        "next": "14.1.3",
        "react-dom": "18.2.0",
        "react": "18.2.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@egoist/tailwindcss-icons": "^1.7.4",
        "@iconify-json/heroicons": "^1.1.20",
        "@iconify-json/lucide": "^1.1.171",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@types/node": "20.11.25",
        "@types/react-dom": "18.2.21",
        "@types/react": "18.2.64",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "autoprefixer": "10.4.18",
        "clsx": "^2.1.0",
        "eslint-config-next": "^14.1.3",
        "eslint-plugin-simple-import-sort": "^12.0.0",
        "eslint": "8.57.0",
        "npm-check-updates": "^16.14.15",
        "postcss-load-config": "^5.0.3",
        "postcss": "8.4.35",
        "prettier-plugin-tailwindcss": "^0.5.12",
        "prettier": "^3.2.5",
        "tailwind-merge": "^2.2.1",
        "tailwindcss-debug-screens": "^2.2.1",
        "tailwindcss": "3.4.1",
        "typescript": "~5.4.2"
    }
}
