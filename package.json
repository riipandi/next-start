{
    "name": "next-start",
    "version": "13.4.2",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
        "deps": "npx --yes npm-check-updates -x typescript --deep --interactive",
        "docker:build": "docker build --compress -t $npm_package_name:$npm_package_version -t $npm_package_name:latest .",
        "docker:run": "docker run --rm --interactive -p 3000:3000 --name $npm_package_name $npm_package_name:latest",
        "docker:shell": "docker exec -it $npm_package_name /bin/sh",
        "docker:logs": "docker logs -f $npm_package_name",
        "docker:stop": "docker stop $npm_package_name"
    },
    "dependencies": {
        "@twistail/icons": "^0.1.0",
        "@twistail/toast": "^0.2.0",
        "next-plausible": "^3.9.1",
        "next": "13.4.9",
        "react-dom": "18.2.0",
        "react": "18.2.0"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@types/node": "20.4.1",
        "@types/react-dom": "18.2.6",
        "@types/react": "18.2.14",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "autoprefixer": "10.4.14",
        "clsx": "^1.2.1",
        "eslint-config-next": "13.4.9",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-tailwindcss": "^3.13.0",
        "eslint": "8.44.0",
        "postcss": "8.4.25",
        "prettier": "^3.0.0",
        "tailwind-merge": "^1.13.2",
        "tailwind-scrollbar-hide": "^1.1.7",
        "tailwindcss": "3.3.2",
        "typescript": "5.0.4"
    }
}
